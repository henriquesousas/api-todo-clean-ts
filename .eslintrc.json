{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": ["airbnb-base", "prettier", "prettier/prettier", "plugin:prettier/recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off"   
    }
}
